  },
  "version": "6.0.0-beta13",
  "license": "Apache-2.0",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/main35/NuxtSite"
  },
  "scripts": {
    "build": "nuxt build",
    "ci": "bun install && bun run build",
    "dev": "nuxi dev --host 0.0.0.0 --port 5173",
    "preview": "bun run build && nuxt preview --port 5174",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "prettier": "prettier --write --ignore-path .prettierignore .",
    "fixPaths": "replace-in-files --string='~/' --replacement='@/' './**/*.{js,ts,vue}'",
    "format": "bun run prettier && bun run fixPaths"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@nuxtjs/i18n": "^10.2.1",
    "cobe": "^0.6.5",
    "floating-vue": "^5.2.2",
    "html2canvas": "^1.4.1",
    "marked": "^15.0.12",
    "postcss-load-config": "^6.0.1",
    "posthog-js": "^1.298.1",
    "vue": "^3.5.26",
    "vue-progressive-blur": "^1.0.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@vite-pwa/nuxt": "^1.1.0",
    "nuxt": "^4.2.2",
    "postcss-import": "^16.1.1",
    "prettier": "^3.7.4",
    "replace-in-files-cli": "^4.0.0",
    "sass": "^1.89.0"
  }
}
